{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./",                          /* Base directory to resolve non-absolute module names. */
    "jsx": "preserve",
    "incremental": true
  },  
  "include": [
    "src/**/*",
    "public/**/*",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/pages"  ],
  "exclude": [
    "node_modules"
  ]
}
